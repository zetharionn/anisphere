{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"cache": true
		},
		"frontend#build": {
			"env": ["VITE_SUPABASE_URL", "VITE_SUPABASE_ANON_KEY"]
		},
		"lint": {
			"dependsOn": ["^lint"],
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": false,
			"persistent": true
		}
	}
}
