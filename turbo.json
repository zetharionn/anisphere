{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"]
		},
		"build:frontend": {
			"env": ["VITE_SUPABASE_URL", "VITE_SUPABASE_ANON_KEY"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": false,
			"persistent": true
		}
	}
}
